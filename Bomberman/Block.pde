class Block {
  PVector pos; // position of block
  
  
  //---------------------------------------------------------------------------------------------------------------------------------------------------------
  // constructor
  
  Block(float posX, float posY){
    pos = new PVector();
    pos.x = posX;
    pos.y = posY;
  }
  
  //---------------------------------------------------------------------------------------------------------------------------------------------------------
  
  void show(){
  fill(200, 190, 90);
  rect(pos.x, pos.y, 100, 100);
  }
  
   
}
